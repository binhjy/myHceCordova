<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.hce" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>HCE Demo</name>
    <description>
        A sample Apache Cordova app demonstrating the Cordova HCE plugin.
    </description>
    <author href="https://github.com/don">
        Don Coleman
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <platform name="android">
        <allow-intent href="market:*" />
        <preference name="android-minSdkVersion" value="19" />
    </platform>
    <plugin name="cordova-plugin-hce" spec="~1.0.0">
        <variable name="AID_FILTER" value="F222222222" />
    </plugin>
    <engine name="android" spec="~7.0.0" />
</widget>
